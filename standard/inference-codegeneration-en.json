{
  "urn": {
    "tags": {
      "op": "codegeneration",
      "language": "en",
      "type": "constrained",
      "in": "media:type=string;v=1;textable;scalar",
      "out": "media:type=llm-inference-output;v=1;textable;keyed"
    }
  },
  "command": "llm_inference",
  "title": "Generate Code in English",
  "cap_description": "Code generation and programming tasks in English",
  "metadata": {},
  "media_specs": {
    "media:type=llm-inference-output;v=1;textable;keyed": {
      "media_type": "application/json",
      "profile_uri": "https://capns.org/schema/llm_inference-output",
      "schema": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "code": {
            "type": "string",
            "minLength": 1,
            "description": "The generated code"
          },
          "language": {
            "type": "string",
            "enum": [
              "rust",
              "python",
              "javascript",
              "typescript",
              "go",
              "java",
              "c",
              "cpp",
              "swift",
              "kotlin",
              "sql",
              "shell",
              "html",
              "css",
              "json",
              "yaml",
              "toml"
            ],
            "description": "Programming language used"
          },
          "tests": {
            "type": "string",
            "description": "Generated unit tests (if requested)"
          },
          "documentation": {
            "type": "string",
            "description": "Generated documentation"
          },
          "explanation": {
            "type": "string",
            "description": "Explanation of the code logic"
          },
          "dependencies": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Required dependencies or imports"
          },
          "complexity_score": {
            "type": "integer",
            "minimum": 1,
            "maximum": 10,
            "description": "Code complexity rating (1-10)"
          },
          "tokens_used": {
            "type": "integer",
            "minimum": 0,
            "description": "Number of tokens consumed for generation"
          },
          "model_used": {
            "type": "string",
            "description": "Model that generated the code"
          },
          "generation_time_ms": {
            "type": "integer",
            "minimum": 0,
            "description": "Generation time in milliseconds"
          }
        },
        "required": [
          "code",
          "language"
        ]
      }
    }
  },
  "accepts_stdin": true,
  "arguments": {
    "required": [
      {
        "name": "prompt",
        "media_urn": "media:type=string;v=1;textable;scalar",
        "arg_description": "Code generation prompt or specification",
        "cli_flag": "--prompt",
        "position": 0,
        "validation": {
          "min_length": 1,
          "max_length": 16384
        }
      }
    ],
    "optional": [
      {
        "name": "language",
        "media_urn": "media:type=string;v=1;textable;scalar",
        "arg_description": "Programming language for code generation",
        "cli_flag": "--language",
        "default_value": "rust",
        "validation": {
          "allowed_values": [
            "rust",
            "python",
            "javascript",
            "typescript",
            "go",
            "java",
            "c",
            "cpp",
            "swift",
            "kotlin",
            "sql",
            "shell",
            "html",
            "css",
            "json",
            "yaml",
            "toml"
          ]
        }
      },
      {
        "name": "context",
        "media_urn": "media:type=string;v=1;textable;scalar",
        "arg_description": "Existing code context or codebase information",
        "cli_flag": "--context",
        "validation": {
          "max_length": 65536
        }
      },
      {
        "name": "style",
        "media_urn": "media:type=string;v=1;textable;scalar",
        "arg_description": "Coding style or framework preferences",
        "cli_flag": "--style",
        "validation": {
          "max_length": 4096
        }
      },
      {
        "name": "max_tokens",
        "media_urn": "media:type=number;v=1;textable;numeric;scalar",
        "arg_description": "Maximum tokens for generated code",
        "cli_flag": "--max-tokens",
        "default_value": 4096,
        "validation": {
          "min": 100,
          "max": 8192
        }
      },
      {
        "name": "include_tests",
        "media_urn": "media:type=boolean;v=1;textable;scalar",
        "arg_description": "Whether to include unit tests",
        "cli_flag": "--include-tests",
        "default_value": false
      },
      {
        "name": "include_comments",
        "media_urn": "media:type=boolean;v=1;textable;scalar",
        "arg_description": "Whether to include code comments",
        "cli_flag": "--include-comments",
        "default_value": true
      },
      {
        "name": "model_spec",
        "media_urn": "media:type=string;v=1;textable;scalar",
        "arg_description": "Model spec to use for code generation",
        "cli_flag": "--model-spec",
        "default_value": "hf:MaziyarPanahi/Mistral-7B-Instruct-v0.3-GGUF?include=*Q8_0*,*.json&exclude=*IQ1*,*IQ2*,*IQ3*,*Q2*,*Q3*",
        "validation": {
          "pattern": ".*:.*",
          "min_length": 4,
          "max_length": 512
        }
      }
    ]
  },
  "output": {
    "media_urn": "media:type=llm-inference-output;v=1;textable;keyed",
    "output_description": "Generated code with optional tests and documentation"
  }
}