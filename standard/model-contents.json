{
  "urn": {
    "tags": {
      "action": "contents",
      "type": "model"
    }
  },
  "version": "1.0.0",
  "command": "contents",
  "cap_description": "Inspect the contents and structure of a model",
  "metadata": {},
  "accepts_stdin": false,
  "arguments": {
    "required": [
      {
        "name": "model",
        "arg_type": "string",
        "arg_description": "Model identifier to inspect",
        "cli_flag": "--model",
        "position": 0,
        "validation": {
          "pattern": "^[^\\0]+$",
          "min_length": 1
        }
      }
    ],
    "optional": [
      {
        "name": "depth",
        "arg_type": "string",
        "arg_description": "Level of detail for contents inspection",
        "cli_flag": "--depth",
        "default_value": "summary",
        "validation": {
          "allowed_values": [
            "summary",
            "detailed",
            "full"
          ]
        }
      },
      {
        "name": "format",
        "arg_type": "string",
        "arg_description": "Output format",
        "cli_flag": "--format",
        "default_value": "json",
        "validation": {
          "allowed_values": [
            "json",
            "text",
            "tree"
          ]
        }
      }
    ]
  },
  "output": {
    "output_type": "object",
    "output_description": "Model contents including files, layers, parameters and metadata",
    "content_type": "application/json"
  }
}