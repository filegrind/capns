{
  "urn": {
    "tags": {
      "op": "contents",
      "type": "model"
    }
  },
  "command": "contents",
  "title": "Inspect Model Contents",
  "cap_description": "Inspect the contents and structure of a model",
  "metadata": {},
  "accepts_stdin": false,
  "arguments": {
    "required": [
      {
        "name": "model",
        "media_spec": "capns:ms:str.v1",
        "arg_description": "Model identifier to inspect",
        "cli_flag": "--model",
        "position": 0,
        "validation": {
          "pattern": "^[^\\0]+$",
          "min_length": 1
        }
      }
    ],
    "optional": [
      {
        "name": "depth",
        "media_spec": "capns:ms:str.v1",
        "arg_description": "Level of detail for contents inspection",
        "cli_flag": "--depth",
        "default_value": "summary",
        "validation": {
          "allowed_values": [
            "summary",
            "detailed",
            "full"
          ]
        }
      },
      {
        "name": "format",
        "media_spec": "capns:ms:str.v1",
        "arg_description": "Output format",
        "cli_flag": "--format",
        "default_value": "json",
        "validation": {
          "allowed_values": [
            "json",
            "text",
            "tree"
          ]
        }
      }
    ]
  },
  "output": {
    "media_spec": "capns:contents-output.v1",
    "output_description": "Model contents including files, layers, parameters and metadata"
  }
}