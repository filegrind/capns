{
  "urn": {
    "tags": {
      "action": "extract_pages",
      "ext": "text",
      "in": "content-type: text/plain; profile=https://capns.org/schema/utf8-text",
      "out": "content-type: application/json; profile=https://capns.org/schema/document-pages"
    }
  },
  "command": "extract-pages",
  "title": "Extract Text Document Pages",
  "cap_description": "Extract pages with text content from text files",
  "metadata": {},
  "accepts_stdin": true,
  "arguments": {
    "required": [
      {
        "name": "file_path",
        "media_spec": "content-type: application/json; profile=https://capns.org/schemas/str",
        "arg_description": "Path to the text file to process",
        "cli_flag": "file_path",
        "position": 0,
        "validation": {
          "pattern": "^[^\\0]+$",
          "min_length": 1
        }
      }
    ],
    "optional": [
      {
        "name": "page_range",
        "media_spec": "content-type: application/json; profile=https://capns.org/schemas/str",
        "arg_description": "Page range to extract (e.g., '1-5' or '10-')",
        "cli_flag": "--page-range",
        "validation": {
          "pattern": "^\\d+(-\\d*)?$"
        }
      },
      {
        "name": "output",
        "media_spec": "content-type: application/json; profile=https://capns.org/schemas/str",
        "arg_description": "Write output to specified file instead of stdout",
        "cli_flag": "--output",
        "validation": {
          "pattern": "^[^\\0]+$"
        }
      }
    ]
  },
  "output": {
    "media_spec": "content-type: application/json; profile=https://capns.org/schemas/obj",
    "output_description": "Structured document pages with text content"
  }
}