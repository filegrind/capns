{
  "urn": {
    "tags": {
      "action": "embeddings_dimensions",
      "out": "content-type: application/json; profile=https://capns.org/schema/uint"
    }
  },
  "command": "dimensions",
  "title": "Query Embedding Dimensions",
  "cap_description": "Get the dimensionality of generated embeddings for a specific model",
  "metadata": {},
  "accepts_stdin": false,
  "arguments": {
    "optional": [
      {
        "name": "model_spec",
        "arg_type": "string",
        "arg_description": "Model spec to use for retrieving embeddings dimensions",
        "cli_flag": "--model-spec",
        "default_value": "hf:MaziyarPanahi/Mistral-7B-Instruct-v0.3-GGUF",
        "validation": {
          "pattern": ".*:.*",
          "min_length": 4,
          "max_length": 512
        }
      }
    ]
  },
  "output": {
    "output_type": "object",
    "output_description": "Dimensionality information for the model",
    "content_type": "application/json",
    "schema": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "dimensions": {
          "type": "integer",
          "minimum": 1,
          "maximum": 100000,
          "description": "Dimensionality of the generated embeddings"
        }
      },
      "required": [
        "dimensions"
      ]
    }
  }
}