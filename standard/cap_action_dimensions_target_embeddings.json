{
  "urn": {
    "tags": {
      "action": "dimensions",
      "target": "embeddings"
    }
  },
  "command": "dimensions",
  "cap_description": "Get the dimensionality of generated embeddings for a specific model",
  "metadata": {},
  "accepts_stdin": false,
  "arguments": {
    "optional": [
      {
        "name": "model",
        "arg_type": "string",
        "arg_description": "Model to use for embeddings generation",
        "cli_flag": "--model",
        "default_value": "hf:sentence-transformers/all-MiniLM-L6-v2",
        "validation": {
          "pattern": ".*:.*",
          "min_length": 4,
          "max_length": 512
        }
      },
      {
        "name": "include_patterns",
        "arg_type": "array",
        "arg_description": "Array of file name patterns to include in the model download",
        "cli_flag": "--include_patterns",
        "default_value": "[\"*.json\", \"*.txt\", \"*.safetensors\", \"*.bin\", \"README*\"]",
        "schema": {
          "type": "array",
          "minItems": 1,
          "maxItems": 50,
          "items": {
            "type": "string",
            "minLength": 5,
            "maxLength": 4096,
            "description": "A file name pattern to include"
          }
        }
      },
      {
        "name": "exclude_patterns",
        "arg_type": "array",
        "arg_description": "Array of file name patterns to exclude in the model download",
        "cli_flag": "--exclude_patterns",
        "default_value": "[\"*fp16*\", \"*test*\", \"*pytorch_model*\"]",
        "schema": {
          "type": "array",
          "minItems": 1,
          "maxItems": 50,
          "items": {
            "type": "string",
            "minLength": 5,
            "maxLength": 4096,
            "description": "A file name pattern to exclude"
          }
        }
      }
    ]
  },
  "output": {
    "output_type": "object",
    "output_description": "Dimensionality information for the model",
    "content_type": "application/json",
    "schema": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "dimensions": {
          "type": "integer",
          "minimum": 1,
          "maximum": 100000,
          "description": "Dimensionality of the generated embeddings"
        }
      },
      "required": [
        "dimensions"
      ]
    }
  }
}