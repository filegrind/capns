{
  "urn": {
    "tags": {
      "action": "list",
      "type": "model"
    }
  },
  "version": "1.0.0",
  "command": "list",
  "cap_description": "List available models and their status",
  "metadata": {},
  "accepts_stdin": false,
  "arguments": {
    "optional": [
      {
        "name": "filter",
        "arg_type": "string",
        "arg_description": "Filter models by pattern",
        "cli_flag": "--filter",
        "validation": {
          "pattern": "^[^\\0]*$"
        }
      },
      {
        "name": "status",
        "arg_type": "string",
        "arg_description": "Filter by model status",
        "cli_flag": "--status",
        "validation": {
          "allowed_values": [
            "loaded",
            "available",
            "downloading",
            "cached",
            "all"
          ]
        }
      },
      {
        "name": "format",
        "arg_type": "string",
        "arg_description": "Output format",
        "cli_flag": "--format",
        "default_value": "json",
        "validation": {
          "allowed_values": [
            "json",
            "text",
            "table"
          ]
        }
      }
    ]
  },
  "output": {
    "output_type": "array",
    "output_description": "List of models with their metadata and status",
    "content_type": "application/json"
  }
}