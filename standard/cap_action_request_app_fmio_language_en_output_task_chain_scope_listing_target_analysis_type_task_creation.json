{
  "urn": {
    "tags": {
      "app": "fmio",
      "action": "request",
      "target": "analysis",
      "scope": "listing",
      "output": "task_chain",
      "language": "en",
      "type": "task_creation"
    }
  },
  "command": "listing_analysis_task",
  "title": "Create Listing Analysis Task Chain",
  "category": "Task Management",
  "cap_description": "Create a complete analysis task chain for a listing (first glance → annotation pass → close read → deep dive)",
  "metadata": {},
  "accepts_stdin": false,
  "arguments": {
    "required": [
      {
        "name": "listing_id",
        "arg_type": "string",
        "arg_description": "ID of the listing to analyze",
        "cli_flag": "--listing-id",
        "validation": {
          "pattern": "[0-9a-f-]{36}"
        }
      },
      {
        "name": "tracked_file_id",
        "arg_type": "string",
        "arg_description": "ID of the tracked file to analyze",
        "cli_flag": "--tracked-file-id",
        "validation": {
          "pattern": "[0-9a-f-]{36}"
        }
      }
    ]
  },
  "output": {
    "output_type": "object",
    "output_description": "Information about the created task chain",
    "content_type": "application/json",
    "schema": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "listing_id": {
          "type": "string",
          "description": "ID of the listing being analyzed"
        },
        "task_type": {
          "type": "string",
          "description": "Type of task chain created"
        }
      },
      "required": [
        "listing_id",
        "task_type"
      ]
    }
  }
}